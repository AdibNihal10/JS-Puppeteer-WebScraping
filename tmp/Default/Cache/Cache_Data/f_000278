    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-B5MYS061R0"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-B5MYS061R0');
    </script>
         
    <script>
    var kod_fakulti = '28';
    $(document).ready(function() {
        ListScholarFacultyDatatable = $('#ListScholarFacultyDashboard').DataTable({
            processing: true,
            searching: true,
            sorting: false,
            paging: true,
            pageLength: 10,
            lengthChange: false,
            info: false,
            responsive: true
        });

        $.ajax({
            type: "POST",
            url : '/Scholar/FacultyGetSenaraiStafAkademik?id='+kod_fakulti,
        }).done(function (data) {
            console.log(data);

            var newArrayStafAkademik = [];
            data.data.forEach(function(elm, i) {
                $newObject = {
                    label:  data.data[i].GROUP_STAFF,
                    value:  data.data[i].TOTAL_STAFF
                }

                newArrayStafAkademik.push($newObject);
            })
            var Morrris_Staf_Akademik = Morris.Donut({
                element: 'staf_akademik_chart_faculties',
                data: newArrayStafAkademik,
                resize: true,
                colors: ['#3d90bf', '#bf783d','#1ab394', '#ed0c0c','#FF5733', '#FFBD33','#193C77','#39FF33','#197754','#541977','#1FE874']
            });

            Morrris_Staf_Akademik.options.data.forEach(function(label, i) {
                var legendItem = $('<span></span>').text( label['label']).prepend('<br><span>&nbsp;</span>');
                legendItem.find('span')
                    .css('backgroundColor', Morrris_Staf_Akademik.options.colors[i])
                    .css('width', '20px')
                    .css('display', 'inline-block')
                    .css('margin', '5px');
                $('#legendAcademicStaff_Faculties').append(legendItem)
            });

            var totalStafAkademik = 0;
            for (var i = 0; i < data.data.length; i++) {
                totalStafAkademik += data.data[i].TOTAL_STAFF << 0;
            }
            $("#TotalStafAkademik").append(totalStafAkademik);

        }).fail(function () { });

        $.ajax({
            type: "POST",
            url : '/Scholar/FacultyGetSenaraiVerifiedPublication?id='+kod_fakulti,
        }).done(function (data) {
            console.log(data);

            var newArrayVerifiedPublication = [];
            data.data.forEach(function(elm, i) {
                $newObject = {
                    label:  data.data[i].PUBLICATION_TYPE,
                    value:  data.data[i].TOTAL_PUBLICATION
                }

                newArrayVerifiedPublication.push($newObject);
            })
            var Morris_VerifiedPublication_Chart = Morris.Donut({
                element: 'verified_publication_chart_Faculties',
                data: newArrayVerifiedPublication,
                resize: true,
                colors: ['#3d90bf', '#bf783d', '#1ab394', '#ed0c0c', '#FF5733',
                    '#FFBD33', '#193C77', '#39FF33', '#197754', '#541977',
                    '#1FE874', '#1F93E8', '#D91FE8', '#6BE81F', '#1FE89C',
                    '#E81F62','#E8401F','#E8A41F','#E81F59','#CC0B83']
            });

            Morris_VerifiedPublication_Chart.options.data.forEach(function(label, i) {
                var legendItem = $('<span></span>').text( label['label']).prepend('<br><span>&nbsp;</span>');
                legendItem.find('span')
                    .css('backgroundColor', Morris_VerifiedPublication_Chart.options.colors[i])
                    .css('width', '20px')
                    .css('display', 'inline-block')
                    .css('margin', '5px');
                $('#legendVerifiedPublicationFaculties').append(legendItem)
            });

            var totalPublication = 0;
            for (var i = 0; i < data.data.length; i++) {
                totalPublication += data.data[i].TOTAL_PUBLICATION << 0;
            }
            $("#TotalPublication").append(totalPublication);

        }).fail(function(){

        });

        $.ajax({
            type: "POST",
            url : '/Scholar/Faculty_GetPrincipalInvestigator?id='+kod_fakulti,
        }).done(function (data) {
            console.log(data);

            var newArrayPrincipalInvestigator = [];
            data.data.forEach(function(elm, i) {
                $newObject = {
                    label:  data.data[i].FUND_NAME,
                    value:  data.data[i].BIL_FUND
                }

                newArrayPrincipalInvestigator.push($newObject);
            })
            var Morris_PrincipalInvestigator_Chart = Morris.Donut({
                element: 'principal_investigator_chart_faculties',
                data: newArrayPrincipalInvestigator,
                resize: true,
                colors: ['#FFBD33', '#193C77', '#39FF33', '#197754', '#541977']
            });

            Morris_PrincipalInvestigator_Chart.options.data.forEach(function(label, i) {
                var legendItem = $('<span></span>').text( label['label']).prepend('<br><span>&nbsp;</span>');
                legendItem.find('span')
                    .css('backgroundColor', Morris_PrincipalInvestigator_Chart.options.colors[i])
                    .css('width', '20px')
                    .css('display', 'inline-block')
                    .css('margin', '5px');
                $('#legendPrincipalInvestigatorFaculties').append(legendItem)
            });

            var totalFund = 0;
            for (var i = 0; i < data.data.length; i++) {
                totalFund += data.data[i].BIL_FUND << 0;
            }
            $("#TotalFund").append(totalFund);
        }).fail(function(){

        });

    });
    </script>

</body>

</html>